<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Start_Date_Comparison</fullName>
    <active>true</active>
    <description>End date cannot be earlier than start date</description>
    <errorConditionFormula>Start_Date__c &gt; End_Date__c
&amp;&amp; NOT( $Setup.Bypass_Automation_Settings__c.Bypass_Validation_Rules__c )</errorConditionFormula>
    <errorMessage>&quot;End Date&quot; cannot be earlier than &quot;Start Date&quot;</errorMessage>
</ValidationRule>
