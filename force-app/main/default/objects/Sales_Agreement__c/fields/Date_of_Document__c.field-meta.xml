<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_of_Document__c</fullName>
    <description>This formula field is to cater dynamic form generation</description>
    <externalId>false</externalId>
    <formula>TEXT(DAY(TODAY())) +&quot;/&quot; +TEXT(MONTH( TODAY() ))+&quot;/&quot; +TEXT(YEAR( TODAY() ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Date of Document</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
